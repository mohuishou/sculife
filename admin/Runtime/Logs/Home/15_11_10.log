SQL: INSERT INTO `webdesign_article` (`ctime`,`tag`,`content`,`number`,`title`) VALUES ('2015-11-10 23:52:34','青春川大','<div class=\"content-art\">
			<h1>关于向广大同学征集江安校区学生创新创业教育实践基地建设意见的通知</h1>
			<div class=\"attr\">
				发布时间：2015-11-05 23:42:00  浏览次数：<script type=\"text/javascript\" src=\"/index.php/system/web/view/viewnumber/id/9169\"></script>
			</div>
			<div class=\"content-text\">
				<div style=\"text-align: center\">
	<span style=\"font-size: 18pt\"><span style=\"font-family: 华文中宋\">关于</span></span><span style=\"font-size: 18pt\"><span style=\"font-family: 华文中宋\">向广大同学征集</span></span><span style=\"font-size: 18pt\"><span style=\"font-family: 华文中宋\">江安校区学生创新创业教育实践基地建设</span></span><span style=\"font-size: 18pt\"><span style=\"font-family: 华文中宋\">意见的通知</span></span></div>
<div>
	<span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">全校同学：</span></span></div>
<div>
	<span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">&nbsp;&nbsp;&nbsp; 目前，学校正在</span></span><span style=\"font-size: 14pt\"><span style=\"color: rgb(75,75,75)\"><span style=\"font-family: 宋体\">全面实施创新创业教育改革</span></span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">，</span></span><span style=\"font-size: 14pt\"><span style=\"color: rgb(43,43,43)\"><span style=\"font-family: 宋体\">把创新创业教育融入人才培养，切实增强学生的创业意识、创新精神和创造能力</span></span></span><span style=\"font-size: 14pt\"><span style=\"color: rgb(43,43,43)\"><span style=\"font-family: 宋体\">。</span></span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">为鼓励和促进大学生创新创业</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">，</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">学校在江安校区商业街进行大学生创新创业和实习实训基地的建设，为同学们学习、生活提供服务保障，为同学们提供实习实训和创新创业能力培育的场所。</span></span></div>
<div>
	<span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">&nbsp;&nbsp;&nbsp; 现邀请</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">各位同学参加调研</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">，</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">旨在</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">进一步了解掌握同学们对于创新</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">创业的认识以及入驻商业街的相关需求，为学校后续工作提供参考。</span></span></div>
<div>
	<span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">&nbsp; &nbsp; 具体</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">参与方式：</span></span></div>
<div>
	<span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">&nbsp; &nbsp;（一）</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">请点击</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">下方</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">网络</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">链接</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">，</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">在问卷星中填写问卷</span></span></div>
<div>
	<span style=\"color: rgb(5,99,193)\"><span style=\"font-family: calibri\">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; <span style=\"font-size:18px;\">&nbsp; &nbsp;<a href=\"http://www.sojump.com/jq/6247376.aspx\">http://www.sojump.com/jq/6247376.aspx</a></span></span></span></div>
<div>
	<span style=\"font-size: 12pt\"><span style=\"font-family: 宋体\">&nbsp; &nbsp; （</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">二）可扫描二维码，用手机填写问卷</span></span></div>
<div style=\"text-align: center;\">
	<span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\"><img alt=\"\" src=\"http://youth.scu.edu.cn/uploads/up/default/-B6-FE-CE-AC-C2-EB.png\" style=\"width: 182px; height: 169px;\" />\" &nbsp; &nbsp; &nbsp;&nbsp;</span></span></div>
<div>
	&nbsp;</div>
<div>
	&nbsp;</div>
<div>
	<span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">&nbsp;&nbsp;&nbsp; 请在11月9日18:00之前</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">填写问卷</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">，本次调研约需用时10分钟，您的积极参与是对学校人才培养和学生创新创业工作</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">的大力</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">支持。</span></span></div>
<div style=\"text-align: right\">
	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">&nbsp;学生工作</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">部</span></span></div>
<div style=\"text-align: right\">
	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">校团委</span></span></div>
<div style=\"text-align: right\">
	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">2015</span></span><span style=\"font-size: 14pt\"><span style=\"font-family: 宋体\">年11月5日</span></span></div>

